<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b3="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b2="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:b3.toggle="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui"
>
    <ui:style>

        .div-panel {
        display: -webkit-box;
        }
        .pro-panel {
        background-color: yellow;
        }
        
    </ui:style>
    <b3:Row>
        <b3:Column size="XS_12">
        
            
            <b2:Modal ui:field="modal" title="Cerrar Quincena" backdrop="STATIC" keyboard="true" animation="true">
                <g:HTMLPanel >
                    <b3.toggle:ToggleSwitch ui:field="toggleSwitch" labelText="Modo" offColor="WARNING" offIcon="USERS" onColor="PRIMARY" onIcon="USER" value="true" />
                    <b3:Button ui:field="btnOK" type="PRIMARY" text="Calcular" />
                </g:HTMLPanel>
            </b2:Modal>

        </b3:Column>

    </b3:Row>
</ui:UiBinder>