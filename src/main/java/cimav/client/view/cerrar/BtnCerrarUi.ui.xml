<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b3="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b3.toggle="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui"
>
    <ui:style>

        .div-panel {
        display: -webkit-box;
        }
        .pro-panel {
        background-color: yellow;
        }
        
    </ui:style>
    
    <g:HTMLPanel >
        <b3.toggle:ToggleSwitch ui:field="toggleSwitch" size="MINI" inverse="true"
                                onIcon="CHECK" offIcon="TIMES"
                                onColor="DANGER" offColor="DEFAULT" value="false" />
        <b3:Button ui:field="btnCerrar" type="DEFAULT" text="Cerrar" enabled="false" size="SMALL" />
    </g:HTMLPanel>
    
</ui:UiBinder>