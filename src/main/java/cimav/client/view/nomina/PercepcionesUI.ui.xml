<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.google.gwt.user.cellview.client"
>
    <ui:style src="nominaUI.css"/>
    
    <g:HTMLPanel addStyleNames="{style.work-panel}">
        
        <g:DockLayoutPanel unit="EM" addStyleNames='{style.dock-panel}'>
            <g:center>
                <c:CellTable ui:field='dataGridPercepciones' addStyleNames='{style.cell-table}'/>
            </g:center>

            <g:south size="3">
                <g:HTML>
                    <h2>SubTotal</h2>
                </g:HTML>
            </g:south>
        </g:DockLayoutPanel>
                
    </g:HTMLPanel>
</ui:UiBinder>