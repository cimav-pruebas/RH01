<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.google.gwt.user.cellview.client"
             xmlns:b3="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:card="urn:import:org.gwtbootstrap3.extras.card.client.ui"
>
    <ui:style src="nominaUI.css"/>
    
    <b3:Panel addStyleNames='{style.conceptos-panel}'>
        <b3:PanelHeader>
            <b3:Heading size="H4" text="Deducciones"/>
        </b3:PanelHeader>
        <b3:PanelBody addStyleNames='{style.conceptos-panel-body}'>
    <!--        <b.html:Strong text="..."/> -->
            <card:Card ui:field='nominaCard' addStyleNames='{style.nomina-card}'>
                <card:Front ui:field='cardBack'> 
                    <c:DataGrid ui:field='dataGridDeducciones' addStyleNames='{style.conceptos-data-grid}'/>
                </card:Front>
                <card:Back>
                    <g:HTML>
                        <h1> Editor ...</h1>
                    </g:HTML>
                </card:Back>
            </card:Card>
        </b3:PanelBody>
<!--        <b3:PanelFooter>
            <b3:Heading size="H3" text="Finals"/>
        </b3:PanelFooter>
        -->
    </b3:Panel>
    
</ui:UiBinder>